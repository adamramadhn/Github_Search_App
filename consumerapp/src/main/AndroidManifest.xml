<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.consumerapp">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="com.example.submission3.READ_DATABASE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_github_logo_foreground"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_github_logo_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Submission3">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
    <queries>
        <package android:name="com.example.submission3" />
    </queries>

</manifest>